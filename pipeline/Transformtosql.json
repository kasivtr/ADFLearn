{
	"name": "Transformtosql",
	"properties": {
		"activities": [
			{
				"name": "Data flow1",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "datafilter",
						"type": "DataFlowReference",
						"parameters": {
							"outputfilenamepatter": {
								"value": "'@{pipeline().parameters.outputfilename}'",
								"type": "Expression"
							}
						},
						"datasetParameters": {
							"source1": {
								"inputstoragefilename": {
									"value": "@pipeline().parameters.inputstorefilename",
									"type": "Expression"
								}
							},
							"sink1": {
								"outputfilename": {
									"value": "@pipeline().parameters.outputfilename",
									"type": "Expression"
								}
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"outputfilename": {
				"type": "string"
			},
			"inputstorefilename": {
				"type": "string"
			}
		},
		"annotations": []
	}
