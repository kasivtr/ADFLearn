{
	"name": "sftpDimProcess",
	"properties": {
		"activities": [
			{
				"name": "copyfromsftpserver",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "SftpReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false,
							"disableChunking": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Info",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "inputblob",
								"type": "LinkedServiceReference"
							},
							"path": "piplelinelog"
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sftpinput",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sftptoblob",
						"type": "DatasetReference",
						"parameters": {
							"folder": "input"
						}
					}
				]
			},
			{
				"name": "loadtoStagingtable",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "copyfromsftpserver",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"additionalColumns": [
							{
								"name": "dw_start_dt",
								"value": {
									"value": "@formatDateTime(utcNow(),'yyyy-MM-dd')",
									"type": "Expression"
								}
							},
							{
								"name": "dw_end_dt",
								"value": {
									"value": "@formatDateTime('9999-12-31','yyyy-MM-dd')",
									"type": "Expression"
								}
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "DimSeller.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "DELETE FROM Staging.DimSeller",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sftptoblob",
						"type": "DatasetReference",
						"parameters": {
							"folder": "input"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "carstable",
						"type": "DatasetReference",
						"parameters": {
							"outtable": "DimSeller"
						}
					}
				]
			},
			{
				"name": "insertnewrecord",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "updatedimtable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "INSERT INTO dbo.DimSeller\nSELECT * FROM [Staging].[DimSeller] WHERE ID NOT IN (SELECT ID FROM dbo.DimSeller WHERE dw_end_dt='9999-12-31');\n\nselect 0 as id;",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "LoadtoAzureSQL",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "updatedimtable",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "loadtoStagingtable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "update t \nset t.dw_end_dt = DATEADD(D,-1,s.dw_start_dt)\nfrom dbo.DimSeller t \n  inner join [Staging].[DimSeller] s \n  on t.id = s.id \n  and  ( t.Value <> s.Value\n  or t.Seller <> s.Seller\n  or t.SellerCode <> s.SellerCode\n  or t.Description <> t.Description )\n  and t.dw_end_dt = '9999-12-31' ;\n\nselect 0 as id;\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "LoadtoAzureSQL",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "move to output",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "loadtoStagingtable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "DimSeller.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sftptoblob",
						"type": "DatasetReference",
						"parameters": {
							"folder": "input"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "sftptoblob",
						"type": "DatasetReference",
						"parameters": {
							"folder": "output"
						}
					}
				]
			}
		],
		"annotations": []
	}
}